{
  "name": "fixit-react-native",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "test": "jest --coverage",
    "test:ci": "jest --env=jsdom --testResultsProcessor=\"jest-junit\" --coverage",
    "eslint": "eslint . --ext .js,.ts,.tsx",
    "eslint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "tsc": "tsc",
    "react-devtools": "react-devtools"
  },
  "dependencies": {
    "@microsoft/signalr": "^5.0.4",
    "@react-native-async-storage/async-storage": "^1.13.4",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/app": "^10.8.0",
    "@react-native-firebase/messaging": "^10.8.0",
    "@react-native-picker/picker": "^1.14.0",
    "@react-navigation/bottom-tabs": "^5.11.7",
    "@react-navigation/native": "^5.9.2",
    "@react-navigation/stack": "^5.14.2",
    "@types/styled-components": "5.1.4",
    "axios": "^0.21.1",
    "base-64": "^1.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "fixit-common-data-store": "^1.0.51-alpha",
    "fixit-common-ui": "^1.0.4-alpha",
    "jwt-decode": "^3.1.2",
    "react": "16.13.1",
    "react-jwt": "^1.1.2",
    "react-native": "0.63.3",
    "react-native-base64": "^0.2.1",
    "react-native-device-info": "^8.0.1",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-msal": "^3.0.5",
    "react-native-ratings": "^7.6.1",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.17.1",
    "react-native-svg": "12.1.0",
    "react-native-swiper-flatlist": "^3.0.14",
    "react-native-vector-icons": "7.1.0",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/runtime": "7.8.4",
    "@react-native-community/eslint-config": "1.1.0",
    "@types/detox": "^17.14.0",
    "@types/jest": "25.2.3",
    "@types/react-native": "0.63.2",
    "@types/react-native-base64": "^0.1.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "16.9.2",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "babel-jest": "25.1.0",
    "detox": "^18.9.0",
    "eslint": "7.15.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.0.1",
    "jest-circus": "26.0.1",
    "jest-junit": "12.0.0",
    "metro-react-native-babel-preset": "0.59.0",
    "react-devtools": "^4.10.1",
    "react-dom": "^17.0.2",
    "react-test-renderer": "16.13.1",
    "react-testing-library": "^8.0.1",
    "redux-mock-store": "^1.5.4",
    "typescript": "4.1.3"
  },
  "jest": {
    "preset": "react-native",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/?!(react-native|native-base)"
    ],
    "coverageReporters": [
      "cobertura",
      "html",
      "lcov"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": "test",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  }
}
