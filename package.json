{
  "name": "fixit-react-native",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android-dev": "npx react-native run-android --variant=devdebug --appIdSuffix=.dev",
    "android-stage": "npx react-native run-android --variant=stagerelease --appIdSuffix=.stage",
    "android-prod": "npx react-native run-android --variant=prodrelease",
    "ios": "npx react-native run-ios --simulator=\"iPhone 13 Pro\"",
    "start": "npx react-native start --reset-cache",
    "test": "jest --coverage",
    "test:ci": "jest --env=jsdom --testResultsProcessor=\"jest-junit\" --coverage",
    "eslint": "eslint . --ext .js,.ts,.tsx",
    "eslint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "ts:watch": "NODE_OPTIONS=\"--max-old-space-size=4096\" tsc --pretty --watch",
    "ts": "tsc --pretty",
    "react-devtools": "react-devtools"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.13.0",
    "@microsoft/applicationinsights-web": "2.6.3",
    "@microsoft/signalr": "5.0.4",
    "@react-native-async-storage/async-storage": "1.13.4",
    "@react-native-community/art": "1.2.0",
    "@react-native-community/cli": "7.0.1",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/push-notification-ios": "1.10.1",
    "@react-native-picker/picker": "1.14.0",
    "@react-navigation/bottom-tabs": "5.11.7",
    "@react-navigation/native": "5.9.2",
    "@react-navigation/stack": "5.14.2",
    "@reduxjs/toolkit": "1.5.1",
    "@types/node": "12.19.8",
    "@types/react-native-push-notification": "7.3.3",
    "@types/react-test-renderer": "16.9.2",
    "@types/redux-mock-store": "1.0.2",
    "@types/styled-components": "5.1.4",
    "axios": "0.21.1",
    "base-64": "1.0.0",
    "enzyme": "3.11.0",
    "fixit-common-ui": "1.0.10-alpha",
    "i18next": "20.3.2",
    "jwt-decode": "3.1.2",
    "react": "17.0.2",
    "react-hook-form": "7.10.0",
    "react-i18next": "11.11.0",
    "react-jwt": "1.1.2",
    "react-magnetic-di": "2.2.6",
    "react-native": "0.65.1",
    "react-native-base64": "0.2.1",
    "react-native-device-info": "8.0.1",
    "react-native-elements": "3.4.2",
    "react-native-gesture-handler": "1.9.0",
    "react-native-get-random-values": "1.7.2",
    "react-native-image-picker": "4.7.3",
    "react-native-maps": "0.30.1",
    "react-native-msal": "4.0.4",
    "react-native-progress": "4.1.2",
    "react-native-push-notification": "8.1.1",
    "react-native-ratings": "7.6.1",
    "react-native-reanimated": "1.13.2",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "2.17.1",
    "react-native-svg": "12.1.0",
    "react-native-swiper-flatlist": "3.0.14",
    "react-native-toast-message": "1.4.9",
    "react-native-vector-icons": "9.0.0",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0",
    "styled-components": "5.2.1",
    "use-async-effect": "2.2.3",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/runtime": "7.12.5",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/detox": "17.14.0",
    "@types/enzyme": "3.10.8",
    "@types/jest": "25.2.3",
    "@types/react-native": "0.65.1",
    "@types/react-native-base64": "0.1.0",
    "@types/react-native-push-notification": "7.3.3",
    "@types/react-redux": "7.1.16",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "babel-jest": "26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "18.9.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "jest-junit": "12.0.0",
    "metro": "0.67.0",
    "metro-react-native-babel-preset": "0.67.0",
    "react-devtools": "4.10.1",
    "react-dom": "17.0.2",
    "react-native-codegen": "0.0.7",
    "react-test-renderer": "17.0.2",
    "react-testing-library": "8.0.1",
    "redux-mock-store": "1.5.4",
    "typescript": "4.1.3"
  },
  "jest": {
    "preset": "react-native",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/?!(react-native|native-base)"
    ],
    "coverageReporters": [
      "cobertura",
      "html",
      "lcov"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": "test",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  }
}
